}, {
  "parameter": "itemHeight",
  "intrduce": "MenuItem高度",
  "type": "number",
  "default": 36
}, {
  "parameter": "itemFontSize",
  "intrduce": "MenuItem文字大小",
  "type": "number",
  "default": 12
}, {
  "parameter": "itemFontFamily",
  "intrduce": "MenuItem字体",
  "type": "string",
  "default": "\"Microsoft yahei\", Tahoma, Verdana, Arial, sans-serif"
}]