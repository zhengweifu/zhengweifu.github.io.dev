[{
  "parameter": "mode",
  "intrduce": "菜单类型，现在支持垂直和水平模式两种",
  "type": "string: horizontal, vertical",
  "default": "horizontal"
}, {
  "parameter": "theme",
  "intrduce": "主题颜色",
  "type": "string: dark, light, custom",
  "default": "dark"
}, {
  "parameter": "bgColor",
  "intrduce": "菜单背景颜色，mode='custom'起作用",
  "type": "string",
  "default": "GREY800"
}, {
  "parameter": "activeLevelIndex",
  "intrduce": "选择到哪一层的ID",
  "type": "number",
  "default": -1
}, {
  "parameter": "activeParentIndex",
  "intrduce": "被选择的上一层ID",
  "type": "number",
  "default": -1
}, {
  "parameter": "activeMemberIndex",
  "intrduce": "被选择的当前层ID",
  "type": "number",
  "default": -1
}, {
  "parameter": "subBgColor",
  "intrduce": "SubMenu背景颜色，mode='custom'起作用",
  "type": "string",
  "default": "GREY900"
}, {
  "parameter": "itemActiveBgColor",
  "intrduce": "MenuItem被选择的颜色，mode='custom'起作用",
